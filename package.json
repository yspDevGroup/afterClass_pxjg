{
  "name": "after-class-pxjg",
  "version": "1.0.0",
  "private": true,
  "description": "课后服务——培训机构端",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "build:dev": "cross-env UMI_ENV=dev umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "openapi": "umi openapi",
    "lint": "eslint src --ext .ts,.tsx,.js,jsx",
    "lint_fix": "eslint src --ext .ts,.tsx,.js,jsx --fix"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/charts": "^1.2.10",
    "@ant-design/pro-layout": "^6.5.0",
    "@ant-design/pro-table": "^2.46.1",
    "@umijs/plugin-openapi": "^1.3.0",
    "antd": "^4.16.11",
    "antd-img-crop": "^3.14.3",
    "braft-editor": "^2.3.9",
    "jquery": "^3.6.0",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "^3.5.15"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "@umijs/preset-react": "^1.8.17",
    "@umijs/test": "^3.5.15",
    "cross-env": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-alloy": "^4.1.0",
    "eslint-plugin-react": "^7.24.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.3.2",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
